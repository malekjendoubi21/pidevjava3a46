<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="main_hook" prefHeight="760.0" prefWidth="1550.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.NewsController">
    <top>
        <HBox alignment="CENTER_LEFT" prefHeight="760.0" prefWidth="1550.0" spacing="20.0" BorderPane.alignment="CENTER">
         <VBox prefHeight="760.0" prefWidth="1550.0">
            <children>
                  <Button mnemonicParsing="false" onAction="#news" prefHeight="55.0" prefWidth="233.0" style="-fx-background-color: #5AAC4E;" text="Return" textFill="WHITE">
                      <styleClass>
                          <String fx:value="pink-button" />
                          <String fx:value="shadow" />
                      </styleClass>
                      <font>
                          <Font name="Andalus" size="24.0" />
                      </font>
                      <cursor>
                          <Cursor fx:constant="HAND" />
                      </cursor>
                  </Button>
               <BorderPane fx:id="hold" prefHeight="705.0" prefWidth="1550.0" />
            </children>
         </VBox>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </HBox>
    </top>
</BorderPane>
